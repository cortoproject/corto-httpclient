in package /corto/httpclient

struct Result::
  status: uint16
  response: string

Result get(string url, string fields)
Result post(string url, string fields)
string encodeFields(string fields)
int16 setTimeout(int32 timeout)
int32 getTimeout()
int16 setConnectTimeout(int32 timeout)
int32 getConnectTimeout()

struct Url::
  address: string
  response: string, private | local
  string request()
